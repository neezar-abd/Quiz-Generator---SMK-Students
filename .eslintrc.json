{
  "root": true,
  "extends": [
    "next/core-web-vitals"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }],
    "@next/next/no-html-link-for-pages": "off"
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "dist/"
  ]
}
